{
  "name": "breathe-together-v2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "typecheck": "tsc",
    "lint": "biome lint ./src ./worker",
    "format": "biome format --write ./src ./worker",
    "format:check": "biome format ./src ./worker",
    "check": "biome check --write ./src ./worker",
    "check:ci": "biome ci ./src ./worker",
    "prepare": "husky"
  },
  "dependencies": {
    "@react-three/drei": "^10.0.6",
    "@react-three/fiber": "^9.1.2",
    "@react-three/postprocessing": "^3.0.4",
    "@tanstack/react-query": "^5.90.12",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/three": "^0.175.0",
    "@vitejs/plugin-react": "^4.4.1",
    "gsap": "^3.14.2",
    "hono": "^4.11.3",
    "koota": "^0.4.0",
    "maath": "^0.10.8",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "simplex-noise": "^4.0.3",
    "three": "^0.175.0",
    "typescript": "^5.8.3",
    "vite": "^6.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.12",
    "msw": "^2.12.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
