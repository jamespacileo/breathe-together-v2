{
  "name": "breathe-together-v2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "typecheck": "tsc",
    "lint": "biome lint ./src",
    "format": "biome format --write ./src",
    "format:check": "biome format ./src",
    "check": "biome check --write ./src",
    "check:ci": "biome ci ./src",
    "prepare": "husky"
  },
  "dependencies": {
    "@react-three/drei": "^10.0.6",
    "@react-three/fiber": "^9.1.2",
    "@react-three/postprocessing": "^3.0.4",
    "@react-three/uikit": "^1.0.60",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/three": "^0.175.0",
    "@vitejs/plugin-react": "^4.4.1",
    "koota": "^0.4.0",
    "maath": "^0.10.8",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "simplex-noise": "^4.0.3",
    "three": "^0.175.0",
    "typescript": "^5.8.3",
    "vite": "^6.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.12"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  }
}
