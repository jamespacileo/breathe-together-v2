{
  "name": "breathe-together-v2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:perf": "vitest run --testNamePattern='Performance|Gate'",
    "typecheck": "tsc",
    "lint": "biome lint ./src",
    "format": "biome format --write ./src",
    "format:check": "biome format ./src",
    "check": "biome check --write ./src",
    "check:ci": "biome ci ./src",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-slider": "^1.3.6",
    "@react-three/drei": "^10.0.6",
    "@react-three/fiber": "^9.1.2",
    "@react-three/uikit": "^1.0.60",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/three": "^0.175.0",
    "@vitejs/plugin-react": "^4.4.1",
    "koota": "^0.4.0",
    "leva": "^0.10.1",
    "lucide-react": "^0.562.0",
    "maath": "^0.10.8",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.18",
    "three": "^0.175.0",
    "tone": "^15.1.22",
    "typescript": "^5.8.3",
    "vite": "^6.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@types/gl": "^6.0.5",
    "@vitest/coverage-v8": "^4.0.16",
    "canvas": "^3.2.0",
    "gl": "^8.1.6",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.12",
    "vitest": "^4.0.16",
    "wrangler": "^4.56.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  }
}
